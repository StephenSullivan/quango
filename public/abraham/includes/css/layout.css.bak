html {height:100%;font-family:arial;}
body {margin:0; padding:0; height:100%; }

#toolbar{width:100%;height:29px;background-color:black;}
#header{width:100%;height:71px;line-height:71px;background-color:gainsboro;border-bottom:1px solid white;}
#logo{margin-left:32px;width:30em;float:left}
#search{margin-right:32px;float:right;}

#header{background-color:#efefef;box-shadow:3px 0 6px #bdbdbd;-webkit-box-shadow:3px 0 6px #bdbdbd;behavior: url(includes/css/PIE.htc);}
#panel{float:left;background-color:#fefefe;width:381px;border-right:1px solid white;-webkit-box-shadow:0 3px 6px #bdbdbd;box-shadow:0 3px 6px #bdbdbd;behavior: url(includes/css/PIE.htc);}
#map{float:right;}
#footer{height:29px;line-height:29px;background-color:black;color:white;}
.footer-inner{margin:0 32px;}