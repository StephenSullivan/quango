.block
  %h3
    = "Recent Members"
  .content
    %ul.list
      -current_group.users.each do |user|
        %li{:onclick => "document.location.href='#{user_path(user)}'"}
          -if user.login != "admin"
            = render :partial => "users/user_mini", :locals => {:user => user, :list_type => 'recent'}
            -#= render :partial => "widgets/member", :locals => {:user => user}
